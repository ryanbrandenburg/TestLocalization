
<section class="content">
    <div class="col-md-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">@ContactLocString["Contacts.Liste"]</h3>
                <div class="box-tools pull-right">
                    <button data-widget="collapse" class="btn btn-box-tool"></button>
                    <button data-widget="remove" class="btn btn-box-tool"></button>
                </div>
            </div>
            <div class="box-body">
                <table id="contacts-table" class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>
                                @ContactLocString["Contact.Prenom"]
                            </th>
                            <th>
                                @ContactLocString["Contact.Nom"]
                            </th>
                            <th>
                                @ContactLocString["Contact.Acces.Date"]
                            </th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
</section>
